{
  "name": "ali-news-api",
  "version": "2.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug $0 --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "typeorm": "typeorm-ts-node-commonjs -d src/modules/database/ormconfig.ts",
    "typeorm:generate": "yarn run typeorm migration:generate src/migrations/$0",
    "typeorm:show": "yarn run typeorm migration:show",
    "typeorm:up": "yarn run typeorm migration:run",
    "typeorm:down": "yarn run typeorm migration:revert",
    "typeorm:drop": "yarn run typeorm schema:drop"
  },
